<!DOCTYPE html>
<html>
  <head>
    <!--Import Google Icon Font-->
    <link
      href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet"
    />
    <!--Import materialize.css-->
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css"
    />
    <link rel="stylesheet" href="public\css\materialize.css" />
    

    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Quacker</title>
  </head>

  <body>
    <!-- nav bar -->
    <nav>
      <div class="nav-wrapper">
        <a href="#" class="brand-logo right"><img class="circle responsive-img" src="public\images\Quacker Logo.png" alt="Quacker Logo"/></a>
        <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">menu</i></a>
        <ul id="nav-mobile" class="left hide-on-med-and-down">
          <li><a href="#">Home</a></li>
          <li><a href="#">LogIn</a></li>
          <li><a href="#">Log Out</a></li>
        </ul>
      </div>
    </nav>

    <main>
      <div class="row">
        <!-- left sidebar -->

        <div class="row p">
            <form class="col s12">
              <div class="row">
                <div class="input-field col s12 m4 l3">
                  <textarea id="textarea1" class="materialize-textarea"></textarea>
                  <label for="textarea1">New Post</label>
                </div>
              </div>
            </form>
          </div>
        <div class="col s12 m4 l3">
          <a class="waves-effect waves-light btn-large"
            ><i class="material-icons left">comment</i>Post</a
          >
        </div>
        <!-- end left -->

        <!-- main -->
        <div class="col s12 m8 l9">
            <div class="row">
                <div class="col s12 m8 l9">
                  <div class="card blue-grey darken-1">
                    <div class="card-content rgb(215, 171, 40)-text">
                      <p>Handlebars.</p>
                    </div>
                  </div>
                </div>
              </div>
        </div>
      </div>
    </main>

    <!--JavaScript at end of body for optimized loading-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
    <script src="./index.js"></script>
  </body>
</html>
