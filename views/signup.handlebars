<h1>
  <a id="Q" href="https://ducklings.io/">Q</a>uacker
</h1>

<div class="row">
  <form class="col s4 push-s4" id="signup-form">
    <div class="input-field">
      <input type="text" id="firstname-input-signup">
      <label>First Name</label>
    </div>
    <div class='input-field'>
      <input type='text' id='lastname-input-signup' />
      <label>Last Name</label>
    </div>
    <div class='input-field'>
      <input type='text' id='username-input-signup' />
      <label>Username</label>
    </div>
    <div class='input-field'>
      <input type='password' id='password-input-signup' class='validate' />
      <label>Password</label>
    </div>
    <div class='input-field'>
      <input type='text' id='email-input-signup' />
      <label>Email</label>
    </div>
    <div class="row center-align">
      <button id="createBtn" class=' btn waves-effect waves-teal ' type='submit' name='action'>Create Account
      </button>
    </div>

  </form>
</div>
<script src='/js/signup.js'></script>